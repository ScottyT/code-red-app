{
  "steps": [
    {
      "name":"gcr.io/cloud-builders/docker",
      "entrypoint": "bash",
      "args":["-c", "docker pull gcr.io/wesi-form/firestore-go:latest || exit 0"]
    },
    {
      "name": "gcr.io/cloud-builders/docker",
      "args": [
        "build",
        "-t",
        "gcr.io/wesi-form/firestore-go:latest",
        "--cache-from",
        "gcr.io/wesi-form/firestore-go:latest",
        "."
      ]
    }
  ],
  "images": ["gcr.io/wesi-form/firestore-go:latest"]
}
