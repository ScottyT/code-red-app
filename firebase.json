{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*.@(jpg|png|gif)",
      "headers": [{
        "key": "Cache-Control",
        "value": "public, max-age=300, s-maxage=3600"
      }]
    }, {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }],
    "rewrites": [
      {
        "source": "*",
        "run": {
          "serviceId": "code-red-app",
          "region": "us-central1"
        }
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  }
}
